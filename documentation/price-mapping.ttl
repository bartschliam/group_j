@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#>.

<#MapDetails>
 
rr:logicalTable [ rr:tableName "HOUSEDATA"];
		
	rr:subjectMap	[
		rr:template "http://data.example.org/House/{STATISTIC}/{YEAR}/{AREA}";
		rr:termType rr:IRI;
		rr:class foaf:HousePrice;
	];		
	
	rr:predicateObjectMap [
		rr:predicate geo:hasGeometry;
		rr:objectMap	[
			rr:template "http://data.example.org/House/{STATISTIC CODE}/{AREA}";
			rr:class geo:Geometry;
			rr:termType rr:IRI;
		];
	];

	rr:predicateObjectMap [
		rr:predicate foaf:Statistic;
		rr:objectMap  [ 
			rr:column "STATISTIC";
			rr:termType rr:Literal;
			rr:datatype xsd:string;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate foaf:Year;
		rr:objectMap  [ 
			rr:column "YEAR";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];

	rr:predicateObjectMap [
		rr:predicate foaf:Value;
		rr:objectMap  [ 
			rr:column "VALUE";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];

	rr:predicateObjectMap [
		rr:predicate foaf:Area;
		rr:objectMap  [ 
			rr:column "AREA";
			rr:termType rr:Literal;
			rr:datatype xsd:string;
		];	
	];
	
 .
 
 <#MapDetailsGeo>

rr:logicalTable [ rr:tableName "HOUSEDATA"];

	rr:subjectMap	[
		rr:template "http://data.example.org/House/{STATISTIC CODE}/{AREA}";
		rr:class geo:Geometry;
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
	rr:predicate geo:asWKT;
	rr:objectMap [ 
		rr:template "POINT({AREA})";
		rr:termType rr:Literal;
		rr:datatype geo:wktLiteral;
		];		
	];

.
