@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#>.

<#MapDetails>
 
rr:logicalTable [ rr:tableName "GARDA_STATIONS"];
		
	rr:subjectMap	[
		rr:template "http://data.example.org/Station/{ID}/{YEAR}";
		rr:termType rr:IRI;
		rr:class foaf:GardaStation;
	];		
	
	rr:predicateObjectMap [
		rr:predicate geo:hasGeometry;
		rr:objectMap	[
			rr:template "http://data.example.org/Station/{ID}/{LONGITUDE}_{LATITUDE}";
			rr:class geo:Geometry;
			rr:termType rr:IRI;
		];
	];

	rr:predicateObjectMap [
		rr:predicate foaf:Year;
		rr:objectMap  [ 
			rr:column "YEAR";
			rr:termType rr:Literal;
			rr:datatype xsd:gYear;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate foaf:Station;
		rr:objectMap  [ 
			rr:column "STATION";
			rr:termType rr:Literal;
			rr:datatype xsd:string;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate foaf:longitude;
		rr:objectMap  [ 
			rr:column "LONGITUDE";
			rr:termType rr:Literal;
			rr:datatype xsd:double;
		];	
	];

	rr:predicateObjectMap [
		rr:predicate foaf:latitude;
		rr:objectMap  [ 
			rr:column "LATITUDE";
			rr:termType rr:Literal;
			rr:datatype xsd:double;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate foaf:Attempted-Murder-Assaults-Harrasments;
		rr:objectMap  [ 
			rr:column "Attempts or threats to murder, assaults, harassments and related offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Dangerous-Negligent-Acts;
		rr:objectMap  [ 
			rr:column "Dangerous or negligent acts";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	
	
	rr:predicateObjectMap [
		rr:predicate foaf:Kidnapping;
		rr:objectMap  [ 
			rr:column "Kidnapping and related offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Robbery;
		rr:objectMap  [ 
			rr:column "Robbery, extortion and hijacking offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Burglary;
		rr:objectMap  [ 
			rr:column "Burglary and related offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Theft;
		rr:objectMap  [ 
			rr:column "Theft and related offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Fraud;
		rr:objectMap  [ 
			rr:column "Fraud, deception and related offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Controlled-Drug-Offences;
		rr:objectMap  [ 
			rr:column "Controlled drug offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Weapons-Explosives;
		rr:objectMap  [ 
			rr:column "Weapons and Explosives Offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Damage-Property;
		rr:objectMap  [ 
			rr:column "Damage to property and to the environment";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Offenses-Against-Government;
		rr:objectMap  [ 
			rr:column "Offences against government, justice procedures and organisation of crime";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Public-Order;
		rr:objectMap  [ 
			rr:column "Public order and other social code offences";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];		
 .
 
 <#MapStationsGeo>

rr:logicalTable [ rr:tableName "GARDA_STATIONS"];

	rr:subjectMap	[
		rr:template "http://data.example.org/Station/{ID}/{LONGITUDE}_{LATITUDE}";
		rr:class geo:Geometry;
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
	rr:predicate geo:asWKT;
	rr:objectMap [ 
		rr:template "POINT({LONGITUDE} {LATITUDE})";
		rr:termType rr:Literal;
		rr:datatype geo:wktLiteral;
		];		
	];

.
