@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix fcc: <http://www.example.org/ont#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo2: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#garda_stations>
	a rr:TriplesMap ;
	
	rr:logicalTable [ rr:tableName "GARDA STATIONS" ] ;
	
	rr:subjectMap [
		rr:template "http://data.example.org/ws/{NAME}" ;
		rr:class geo:Feature ;
		rr:class fcc:Division ;
	] ;
	
	rr:predicateObjectMap [
		rr:predicate rdfs:label ;
		rr:objectMap [
			rr:column "Divisions" ; rr:language "en" ;
		] ; 
		rr:objectMap [
			rr:column "Divisions" ;
		] ; 
	] ;
 	
	rr:predicateObjectMap [
		rr:predicate fcc:mah ;
		rr:objectMap [ 
			rr:column "Attempts or threats to murder, assaults, harassments and related offences" ; 
			rr:datatype xsd:integer ; 
		] ; 
	] ;

	rr:predicateObjectMap [
		rr:predicate fcc:dn ;
		rr:objectMap [ 
			rr:column "Dangerous or negligent acts" ; 
			rr:datatype xsd:integer ; 
		] ; 
	] ;

