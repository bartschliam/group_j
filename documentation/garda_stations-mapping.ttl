@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#>.

<#MapDetails>
 
rr:logicalTable [ rr:tableName "GARDA_STATIONS"];
		
	rr:subjectMap	[
		rr:template "http://foo.example.org/Station/{ID}/{YEAR}";
		rr:termType rr:IRI;
		rr:class foaf:GardaStation;
	];		
	
	rr:predicateObjectMap [
		rr:predicate geo:hasGeometry;
		rr:objectMap	[
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}/{LONGITUDE}_{LATITUDE}";
			rr:class geo:Geometry;
			rr:termType rr:IRI;
		];
	];

	rr:predicateObjectMap [
		rr:predicate foaf:Year;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}";
			rr:termType rr:Literal;
			rr:datatype xsd:gYear;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate foaf:Station;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}/{STATION}";
			rr:termType rr:Literal;
			rr:datatype xsd:string;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate foaf:longitude;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}/{LONGITUDE}";
			rr:termType rr:Literal;
			rr:datatype xsd:double;
		];	
	];

	rr:predicateObjectMap [
		rr:predicate foaf:latitude;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}/{LATITUDE}";
			rr:termType rr:Literal;
			rr:datatype xsd:double;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate foaf:Attempted-Murder-Assaults-Harrasments;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}/{Attempts or threats to murder, assaults, harassments and related offences}";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Dangerous-Negligent-Acts;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}/{Attempts or threats to murder, assaults, harassments and related offences}";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	
	
	rr:predicateObjectMap [
		rr:predicate foaf:Kidnapping;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}/{Kidnapping and related offences}";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Robbery;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Station/{ID}/{YEAR}/{Robbery, extortion and hijacking offences}";
			rr:termType rr:Literal;
			rr:datatype xsd:integer;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:Burglary;
