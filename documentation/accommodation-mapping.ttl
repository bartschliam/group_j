@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#>.

<#MapDetails>
 
rr:logicalTable [ rr:tableName "ACCOMMODATION"];
		
	rr:subjectMap	[
		rr:template "http://foo.example.org/Accommodation/{NAME}";
		rr:termType rr:IRI;
		rr:class foaf:Accom;
	];		
	
	rr:predicateObjectMap [
		rr:predicate geo:hasGeometry;
		rr:objectMap	[
			rr:template "http://foo.example.org/Accommodation/{NAME}/{LONGITUDE}_{LATITUDE}";
			rr:class geo:Geometry;
			rr:termType rr:IRI;
		];
	];

	rr:predicateObjectMap [
		rr:predicate foaf:name;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Accommodation/{NAME}/{TAGS}";
			rr:termType rr:Literal;
			rr:datatype xsd:string;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate foaf:AddressLocality;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Accommodation/{NAME}/{ADDRESSLOCALITY}";
			rr:termType rr:Literal;
			rr:datatype xsd:string;
		];	
	];	

	rr:predicateObjectMap [
		rr:predicate foaf:AddressRegion;
		rr:objectMap  [ 
			rr:template "http://foo.example.org/Accommodation/{NAME}/{ADDRESSREGION}";
			rr:termType rr:Literal;
			rr:datatype xsd:string;
		];	
	];	
 .