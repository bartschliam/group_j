@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#>.

#Example
@prefix ex: <http://data.example.org/>



<#MapDetails>
 
rr:logicalTable [ rr:tableName "ACCOMMODATION"];
		
	rr:subjectMap	[
		rr:template "http://data.example.org/Accommodation/{NAME}";
		rr:termType rr:IRI;
		rr:class sosa:Sampler;
	];		
	
	rr:predicateObjectMap [
		rr:predicate geo:hasGeometry;
		rr:objectMap	[
			rr:template "http://data.example.org/Accommodation/{NAME}/{LONGITUDE}_{LATITUDE}";
			rr:class geo:Geometry;
			rr:termType rr:IRI;
		];
	];
	
	rr:predicateObjectMap [
		rr:predicate ex:AddressRegion;
		rr:objectMap  [ 
			rr:column "ADDRESSREGION";
			rr:termType rr:Literal;
			rr:datatype xsd:float;
		];	
	];
	
	rr:predicateObjectMap [
		rr:predicate ex:AddressLocality;
		rr:objectMap  [ 
			rr:column "ADDRESSLOCALITY";
			rr:termType rr:Literal;
		];	
	];
	
 .
 
 <#MapDetailsGeo>

rr:logicalTable [ rr:tableName "ACCOMMODATION"];

	rr:subjectMap	[
		rr:template "http://data.example.org/Accommodation/{NAME}/{LONGITUDE}_{LATITUDE}";
		rr:class geo:Geometry;
		rr:termType rr:IRI;
	];

	rr:predicateObjectMap [
	rr:predicate geo:asWKT;
	rr:objectMap [ 
		rr:template "POINT({LONGITUDE} {LATITUDE})";
		rr:termType rr:Literal;
		rr:datatype geo:wktLiteral;
		];		
	];

.
